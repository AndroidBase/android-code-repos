<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.anduo.intent"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <application
        android:name="xyz.anduo.intent.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="xyz.anduo.intent.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="xyz.anduo.intent.Other1Activity"></activity>
        <activity android:name="xyz.anduo.intent.Other2Activity"></activity>
        <activity android:name="xyz.anduo.intent.Other3Activity"></activity>
        <activity android:name="xyz.anduo.intent.Other4Activity"></activity>
        <activity android:name="xyz.anduo.intent.Other5Activity"></activity>
    </application>

</manifest>
